{
  // "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": false,
    "checkJs": false,
    "outDir": "./dist",
    "strict": true,
    "declaration": false,
    "esModuleInterop": true,
    "allowUmdGlobalAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    // "paths": {
		// 	"rollup": ["./node_modules/rollup"]
		// },
    // "types": [
    //   // "vite/client"
    // ],
  },
  "include": ["**/*.ts", "rollup.config.ts"],
}
