{
  // "extends": "@vue/tsconfig/tsconfig.json",
  "include": ["**/*.ts", "rollup.config.ts"],
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": false,
    "checkJs": false,
    "outDir": "./dist",
    "strict": true,
    "declaration": false,
    "esModuleInterop": true,
    "allowUmdGlobalAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots": ["node_modules/@types"],
  },
}
