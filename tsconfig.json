{
  // "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "DOM"],
    "module": "esnext",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": true,
    "checkJs": false,
    "outDir": "./dist",
    "strict": true,
    "declaration": false,
    "esModuleInterop": true,
    // "types": [
    //   "vite/client"
    // ],
  },
  "include": ["**/*.ts", "**/*.js", "**/*.tsx", "**/*.jsx"],
}
