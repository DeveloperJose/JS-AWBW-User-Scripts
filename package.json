{
  "name": "js-awbw-user-scripts",
  "version": "1.0.0",
  "description": "My custom user scripts for Advance Wars by Web written in JS",
  "private": true,
  "type": "module",
  "sideEffects": [
    "*/*.css"
  ],
  "scripts": {
    "build:dev": "vite build -- --inputDir=music_player",
    "build:music_player": "vite build -- --inputDir=music_player",
    "build:highlight_cursor_coordinates": "vite build -- --inputDir=highlight_cursor_coordinates",
    "build:all": "npm run build:music_player && npm run build:highlight_cursor_coordinates && npm run prettier && npm run lint",
    "netlify": "cd ../JS-Website && npm run netlify",
    "netlify:dev": "cd ../JS-Website && netlify deploy --dir=./public",
    "prettier": "prettier --write \"**/*.{js,ts,css}\"",
    "clean": "rm dist/*.js",
    "lint": "eslint",
    "depcheck": "depcheck",
    "knip": "knip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeveloperJose/JS-AWBW-User-Scripts.git"
  },
  "author": "DeveloperJose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DeveloperJose/JS-AWBW-User-Scripts/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/can-autoplay": "^3.0.5",
    "@types/node": "^22.10.10",
    "@types/spark-md5": "^3.0.5",
    "@types/vue": "^1.0.31",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "can-autoplay": "^3.0.2",
    "depcheck": "^1.4.7",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "globals": "^15.13.0",
    "knip": "^5.43.3",
    "node": "^18.20.6",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rollup": "^4.29.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-external-globals": "^0.13.0",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "spark-md5": "^3.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.11",
    "vite-plugin-lib-inject-css": "^2.2.1",
    "vite-plugin-userscript": "github:DeveloperJose/vite-plugin-userscript",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  }
}
